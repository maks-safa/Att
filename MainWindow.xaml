<Window x:Class="Secvice.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Secvice"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Background="#FFE1E4FF">
        <Grid.RowDefinitions>
            <RowDefinition Height="50"/>
            <RowDefinition/>
            <RowDefinition Height="25"/>
        </Grid.RowDefinitions>
        <TextBlock Grid.Row="0" x:Name="ss"/>
        <Image Grid.Row="0" HorizontalAlignment="Left" VerticalAlignment="Center" Source="Resouse/beauty_logo.png" />
        <Frame x:Name="NavFrame" Grid.Row="1" Background="White"/>
    </Grid>
</Window>
